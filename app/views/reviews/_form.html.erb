<%= form_for([@user, @review]) do |f| %>

	<label>Rating</label>
	<div id="rating-form" data-no-turbolink>
	</div>
  <br>
  <div class="field">
    <%= f.label :comment %><br>
    <%= f.text_area :comment %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
<!-- Script tag allows us to enter ratings as a star and update -->
<script>
  	$('#rating-form').raty({
  		path: '/assets/',
  		scoreName: 'review[rating]'
  	});
</script>